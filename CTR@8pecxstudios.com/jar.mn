[.] chrome.jar:
 content/	                                           						(content/*)
 locale/                                             						    (locale/*)
 ../defaults/                                       						    (defaults/*)
 
% content	classic_theme_restorer								%content/
% skin    classic_theme_restorer	classic/1.0				    %skin/

% locale	classic_theme_restorer	cs							%locale/cs/
% locale	classic_theme_restorer	da							%locale/da/
% locale	classic_theme_restorer	de							%locale/de/
% locale	classic_theme_restorer	dsb						%locale/dsb/
% locale	classic_theme_restorer	el							%locale/el/
% locale	classic_theme_restorer	en-US					%locale/en-US/
% locale	classic_theme_restorer	es							%locale/es/
% locale	classic_theme_restorer	et							%locale/et/
% locale	classic_theme_restorer	fr							%locale/fr/
% locale	classic_theme_restorer	hsb						%locale/hsb/
% locale	classic_theme_restorer	it							%locale/it/
% locale	classic_theme_restorer	ja							%locale/ja/
% locale	classic_theme_restorer	pl							%locale/pl/
% locale	classic_theme_restorer	pt-BR					%locale/pt-BR/
% locale	classic_theme_restorer	ru							%locale/ru/
% locale	classic_theme_restorer	sl							%locale/sl/
% locale	classic_theme_restorer	uk							%locale/uk/
% locale	classic_theme_restorer	zh-CN					%locale/zh-CN/
% locale	classic_theme_restorer	zh-TW					%locale/zh-TW/

% skin	ctraddon_os_special	classic_theme_restorer	%content/css/winaero/	os=WINNT osversion=6.0
% skin	ctraddon_os_special	classic_theme_restorer	%content/css/winaero/	os=WINNT osversion=6.1
% skin	ctraddon_os_special	classic_theme_restorer	%content/css/win8/		os=WINNT osversion>=6.2

% skin	ctraddon_os_special	classic_theme_restorer	%content/css/linux/		os=Linux
% skin	ctraddon_os_special	classic_theme_restorer	%content/css/linux/		os=FreeBSD
% skin	ctraddon_os_special	classic_theme_restorer	%content/css/linux/		os=OpenBSD
% skin	ctraddon_os_special	classic_theme_restorer	%content/css/linux/		os=NetBSD
% skin	ctraddon_os_special	classic_theme_restorer	%content/css/linux/		os=SunOS

% overlay chrome://browser/content/browser.xul			     chrome://classic_theme_restorer/content/overlay.xul
% overlay about:newtab												 chrome://classic_theme_restorer/content/aboutnewtab.xul
% overlay about:preferences											 chrome://classic_theme_restorer/content/aboutprefs.xul

% overlay chrome://browser/content/browser.xul			chrome://classic_theme_restorer/content/overlayWin.xul os=WINNT
% overlay chrome://browser/content/browser.xul			chrome://classic_theme_restorer/content/overlayLin.xul os=Linux
% overlay chrome://browser/content/browser.xul			chrome://classic_theme_restorer/content/overlayLin.xul os=FreeBSD
% overlay chrome://browser/content/browser.xul			chrome://classic_theme_restorer/content/overlayLin.xul os=OpenBSD
% overlay chrome://browser/content/browser.xul			chrome://classic_theme_restorer/content/overlayLin.xul os=NetBSD
% overlay chrome://browser/content/browser.xul			chrome://classic_theme_restorer/content/overlayLin.xul os=SunOS